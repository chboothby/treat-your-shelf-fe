        setBooks(books);\n        }\n      });\n  };\n\n  useEffect(() => {\n    if (data !== \"Not Found\") {\n      getBookByISBN(data);\n    }\n  });\n\n  return (\n    <div>\n      {error ? (\n        <>\n          <p>No books found</p>\n          <Link to=\"/scan\">\n            <Button>Scan Again</Button>\n          </Link>\n        </>\n      ) : data === \"Not Found\" ? (\n        <section className=\"scanner\">\n          <h3>Please scan your book's ISBN number</h3>\n          <BarcodeScannerComponent\n            width={400}\n            height={400}\n            onUpdate={(err, result) => {\n              if (result) {\n                setData(result.text);\n              }\n            }}\n          />\n        </section>\n      ) : (\n        <div className=\"scan-results\">\n          <Link to=\"/scan\">\n            <Button style={{ border: \"solid black 0.5px\" }}>\n              Scan another book\n            </Button>\n          </Link>\n\n          {books.map((book) => {\n            return (\n              <div key={book.id} className=\"book-card\">\n                <img\n                  src={book.images?.thumbnail}\n                  alt={`${book.title}'s cover art`}\n                ></img>\n                <div className=\"book-info\">\n                  <p>Title: {book.title}</p>\n                  <p>Author(s): {book.authors.map((author) => author)}</p>\n                  <p>Published: {book.publishedDate}</p>\n                  {!clicked ? (\n                    <Button\n                      onClick={() => {\n                        handleClick(book);\n                      }}\n                      style={{ background: \"white\" }}\n                    >\n                      Add to bookshelf\n                    </Button>\n                  ) : (\n                    <Button style={{ background: \"green\", color: \"white\" }}>\n                      Added!\n                    </Button>\n                  )}\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default Scanner;\n","/Users/charlesfoy/northcoders/project/treat-yo-shelf-fe/src/Components/Bookshelf.jsx",[],"/Users/charlesfoy/northcoders/project/treat-yo-shelf-fe/src/Components/SimpleBottomNavigation.jsx",["68"],"import React from \"react\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport BottomNavigation from \"@material-ui/core/BottomNavigation\";\nimport BottomNavigationAction from \"@material-ui/core/BottomNavigationAction\";\nimport MenuBookIcon from \"@material-ui/icons/MenuBook\";\nimport SearchIcon from \"@material-ui/icons/Search\";\nimport ChatIcon from \"@material-ui/icons/Chat\";\nimport AccountBoxIcon from \"@material-ui/icons/AccountBox\";\nimport { Link } from \"@reach/router\";\n\nconst useStyles = makeStyles({\n  root: {\n    width: \"100%\",\n    position: \"fixed\",\n    bottom: 0,\n  },\n});\n\nexport default function SimpleBottomNavigation() {\n  const classes = useStyles();\n  const [value, setValue] = React.useState(0);\n\n  return (\n    <BottomNavigation\n      value={value}\n      onChange={(event, newValue) => {\n        setValue(newValue);\n      }}\n      showLabels\n      className={classes.root}\n    >\n      <BottomNavigationAction\n        component={Link}\n        to=\"/\"\n        label=\"Bookshelf\"\n        icon={<MenuBookIcon />}\n      />\n\n      <BottomNavigationAction\n        component={Link}\n        to=\"/search\"\n        label=\"Search\"\n        icon={<SearchIcon />}\n      />\n      <BottomNavigationAction\n        component={Link}\n        to=\"/messages\"\n        label=\"Messages\"\n        icon={<ChatIcon />}\n      />\n      {/* <BottomNavigationAction\n        component={Link}\n        to=\"/help\"\n        label=\"Help\"\n        icon={<HelpOutlineIcon />}\n      />\n      <BottomNavigationAction\n        component={Link}\n        to=\"/account\"\n        label=\"Account\"\n        